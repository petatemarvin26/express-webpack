{
  "name": "express-webpack",
  "version": "1.1.0",
  "description": "This project can use as boilerplate of express backend service application.",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rm -rf ./build",
    "build:dev": "npm run clean & webpack --config ./config/webpack.config.js",
    "build:uat": "npm run clean & webpack --env env=uat --config ./config/webpack.config.js",
    "build:prod": "npm run clean & webpack --env env=prod --config ./config/webpack.config.js",
    "start:dev": "node build/index.js",
    "start:uat": "node build/index.js",
    "start:prod": "node build/index.js",
    "dev": "npm run build:dev && npm run start:dev",
    "uat": "npm run build:uat && npm run start:uat",
    "prod": "npm run build:prod && npm run start:prod",
    "start": "nodemon src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^9.1.3",
    "dotenv": "^16.4.5",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2"
  }
}
